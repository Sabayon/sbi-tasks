{
  "script": [
    "/usr/bin/tini -s -- /sabayon-stuff/sabayon_molecules.sh run daily --image spinbase --skip-docker-rmi --skip-dev" ],
  "directory": "/",
  "task": "docker_execute",
  "image": "geaaru/isobuilder-amd64",
  "storage": "8925468933589065900",
  "artefact_path": "namespace",
  "storage_path": "/confs",
  "tag_namespace": "spinbase:iso",
  "publish_mode": "append",
  "environment": [
    "SABAYON_MOLECULES_GITURL=https://github.com/Sabayon/molecules.git",
    "SABAYON_MOLECULES_SYSTEMD_SLEEP=20",
    "SABAYON_MOLECULES_SYSTEMD_MODE=1",
    "SABAYON_KERNEL_VERSION=4.14",
    "SABAYON_MOLECULES_SCRIPT=/sabayon/scripts/sabayon_iso_build.sh",
    "SABAYON_USE_IMG=0",
    "DRACUT=1",
    "DRACUT_SQUASHFS=0",
    "SABAYON_EXTRA_PKGS=sys-kernel/linux-sabayon:4.14",
    "SABAYON_INSTALL_KERNEL=1",
    "MOLECULE_ISO_TITLE=SABAYON"
  ],
  "binds": [
    "/dev/loop-control:/dev/loop-control",
    "/tmp:/run"
  ],
  "queue": "iso",
  "retry": "5"
}
